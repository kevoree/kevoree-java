<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <name>Kevoree :: Library :: Node :: Java</name>
    <artifactId>java</artifactId>
    <packaging>jar</packaging>

    <parent>
        <groupId>org.kevoree.library.node</groupId>
        <artifactId>node-root</artifactId>
        <version>6.0.0-SNAPSHOT</version>
    </parent>

    <dependencies>
        <dependency>
            <groupId>org.kevoree.tool</groupId>
            <artifactId>injector</artifactId>
            <version>${kevoree.version}</version>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.11</version>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.kevoree.tool</groupId>
                <artifactId>kevoree-maven-plugin</artifactId>
                <configuration>
                    <namespace>kevoree</namespace>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
